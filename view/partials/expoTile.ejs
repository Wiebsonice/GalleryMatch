<html lang=“nl”>
<% for (var index = 0; index < data.expoWishlist.length; index++){ %>
   <li class="expo-tile-single">
       <div class="expotile-bg" style="background-image: url(/assets/expo-covers/<%= data.expoWishlist[index].cover %>)"></div>
       <header>
           <span style=“font-size:10px; word-break:break-all;“></span>
           <h2><%= data.expoWishlist[index].artist %></h2>
           <p><%= data.expoWishlist[index].location %></p>
       </header>
       <a href="/art-galleries/<%= data.expoWishlist[index]._id %>"></a>
   </li>
<% } %>
